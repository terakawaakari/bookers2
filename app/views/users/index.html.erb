<div class="container">
  <div class="row">
    <%= render "books/left", book:@book %>
    <div class="col-md-8">
      <h2>Books</h2>
      <% @users.each do |user| %>
      <table>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
        <tr>
          <td>image</td>
          <td><%= user.name %></td>
          <td><%= link_to "Show",user_path(user.id) %></td>
        </tr>
      </table>
      <% end %>
      <%= paginate @users %>
    </div>
  </div>
</div>