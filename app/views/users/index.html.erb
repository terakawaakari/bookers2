<div class="container">
  <div class="row">
    <%= render "books/left", book:@book, user_id:@user_id %>
    <div class="col-md-8">
      <h2>Users</h2>
      <% @users.each do |user| %>
      <table>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
        <tr>
          <td><%= attachment_image_tag @user_id, :profile_image, size: "30x30", format:'jpeg', fallback:"no_image.jpg" %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show",user_path(user.id) %></td>
        </tr>
      </table>
      <% end %>
      <%= paginate @users %>
    </div>
  </div>
</div>